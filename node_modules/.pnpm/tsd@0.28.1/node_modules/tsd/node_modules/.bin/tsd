#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/tsd@0.28.1/node_modules/tsd/dist/node_modules:/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/tsd@0.28.1/node_modules/tsd/node_modules:/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/tsd@0.28.1/node_modules:/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/tsd@0.28.1/node_modules/tsd/dist/node_modules:/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/tsd@0.28.1/node_modules/tsd/node_modules:/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/tsd@0.28.1/node_modules:/Users/Shared/gnostr.org/public/gnostr/web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cli.js" "$@"
fi
